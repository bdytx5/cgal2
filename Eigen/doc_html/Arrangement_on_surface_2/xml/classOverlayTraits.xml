        </param>
        <param>
          <type><ref refid="classOverlayTraits_1affeba6407709a51771b9b66de1551e94" kindref="member">Face_handle_B</ref></type>
          <declname>f2</declname>
        </param>
        <param>
          <type><ref refid="classOverlayTraits_1a8ab679f5e84bb2ee44f02fdb781ac309" kindref="member">Face_handle_R</ref></type>
