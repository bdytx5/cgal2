        <argsstring>(AlgebraicKernel_d_2::Polynomial_2 f, AlgebraicKernel_d_2::Polynomial_2 g, AlgebraicKernel_d_2::Bound x_l, AlgebraicKernel_d_2::Bound x_u, AlgebraicKernel_d_2::Bound y_l, AlgebraicKernel_d_2::Bound y_u)</argsstring>
