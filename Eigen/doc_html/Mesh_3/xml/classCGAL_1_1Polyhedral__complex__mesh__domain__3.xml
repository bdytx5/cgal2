        <name>detect_features</name>
        <param>
          <type><ref refid="classCGAL_1_1Polyhedral__complex__mesh__domain__3_1af5894b623a9872ff12647d92167bb323" kindref="member">FT</ref></type>
          <declname>angle_bound</declname>
